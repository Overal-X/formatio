		"@kubb/plugin-oas": "^3.5.11",
		"@kubb/plugin-ts": "^3.5.11",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@tanstack/svelte-query-devtools": "^5.66.3",
		"@testing-library/jest-dom": "^6.6.3",
