		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0",
		"vitest": "^3.0.0"
	},
	"dependencies": {
		"@icons-pack/svelte-simple-icons": "^4.0.2",
		"@kubb/plugin-svelte-query": "^3.5.11",
		"@tanstack/svelte-query": "^5.66.0",
